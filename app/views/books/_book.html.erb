<tr>
		<td class="listbox-entry">
			<%= link_to book.title, book %>
		</td>
		<td class="listbox-entry" style="word-break: normal;" align="center">
		  <%= book.author %>
		</td>
		<td class="listbox-entry" width="70" align="center">
		  <%= book.publish_date.strftime("%Y-%m-%d") %>
		</td>
		<td class="listbox-header" width="30" align="center">
		  <%= book.price %>
		</td>
</tr>
